dn: olcDatabase={1}mdb,cn=config
changetype: modify
replace: olcAccess
olcAccess: {0}to attrs=userPassword
  by self write
  by anonymous auth
  by dn.exact="cn=admin,dc=example,dc=com" write
  by * none
olcAccess: {1}to *
  by dn.exact="cn=admin,dc=example,dc=com" manage
  by users read
  by * none
